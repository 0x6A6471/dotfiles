# Zenbones Dark Mode Colors for Fish Shell

# Syntax highlighting colors
set fish_color_command 5599ff          # sky (blue) - commands
set fish_color_keyword fb4934          # rose (red) - keywords
set fish_color_string 6db838           # leaf (green) - strings
set fish_color_operator d3869b         # blossom (magenta) - operators
set fish_color_comment 666666          # gray - comments
set fish_color_error fb4934            # rose (red) - errors
set fish_color_escape fabd2f           # wood (orange) - escape sequences
set fish_color_quote 6db838            # leaf (green) - quoted strings
set fish_color_redirection 5599ff      # sky (blue) - redirections
set fish_color_end d3869b              # blossom (magenta) - end keywords
set fish_color_param ebdbb2            # fg (light gray) - parameters
set fish_color_valid_path --underline  # underline valid paths
set fish_color_autosuggestion 555555   # bright black - autosuggestions
set fish_color_selection --background=333333  # selection highlight
set fish_color_search_match --background=333333  # search match highlight
set fish_color_history_current --bold  # history current item
set fish_color_user 5599ff             # user in prompt
set fish_color_host 5599ff             # host in prompt
set fish_color_status fb4934           # status in prompt (red for errors)
set fish_color_cwd 6db838              # current directory (green)
